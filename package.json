{
  "name": "propolis",
  "version": "0.3.16",
  "private": false,
  "description": "A simple blog platform, written in Preact",
  "repository": {
    "type": "git",
    "url": "https://github.com/tireymorris/propolis.git"
  },
  "bugs": "https://github.com/tireymorris/propolis/issues",
  "author": "tireymorris",
  "main": "src/index.tsx",
  "scripts": {
    "clean": "rm -rf build/",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:dev": "NODE_ENV=development webpack --config webpack.dev.config.js",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.dev.config.js",
    "compile": "NODE_ENV=development yarn run tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "highlight.js": "^10.4.1",
    "history": "^4.10.1",
    "marked": "^0.8.0",
    "preact": "^10.2.1",
    "preact-router": "^3.1.0"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/history": "^4.7.7",
    "@types/marked": "^0.7.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "autoprefixer": "^9.5.1",
    "eslint": "^7.5.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^5.0.2",
    "prettier": "^2.0.5",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
